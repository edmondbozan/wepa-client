{
  "expo": {
    "name": "Wepa",
    "slug": "Wepa",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wepa",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.finelycrafted.wepa",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to allow you to select photos for creating projects that can be shared and viewed by other users in the app. Your photos will only be uploaded and used with your permission."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.finelycrafted.wepa"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c13e0cfe-808d-40bd-adc4-9c95bc60e305"
      }
    }
  }
}
